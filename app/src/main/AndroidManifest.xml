    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity_add_wallpapers.ActivityAddWallpapers"></activity>
        <activity android:name=".activity_wallpapers.ActivityWallpapers" />
        <activity android:name=".activity_add_category.ActivityAddCategory" />
